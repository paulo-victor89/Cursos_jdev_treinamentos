package pvictor.executavel.mdl8;

public class SwitchCase {

	public static void main(String[] args) {
		int nota1 = 7;
		int nota2 = 7;
		int nota3 = 7;
		int nota4 = 7;

		int soma = nota1 + nota2 + nota3 + nota4;
		int media = soma / 4;

		
		/* utilizado para operações exatas */
		switch (media) {
		case 7:
			System.out.println("Aluno aprovado" + media);
			break;

		default:System.out.println("Outro valor:  " + media);
			break;
		}

	}

}
