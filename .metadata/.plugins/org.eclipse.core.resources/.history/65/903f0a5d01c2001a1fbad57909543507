package pvictor.executavel.mdl8;

public class For {

	public static void main(String[] args) {

		for (int numero = 0; numero <= 10; numero++) {
			System.out.println("Ordem crescente é: " + numero);
		}
		for(int numeroDec =10;numeroDec>=0;numeroDec--) {
			System.out.println("Ordem decrescente é: " + numeroDec);
		}
	}

}
